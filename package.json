{
  "name": "onecardswiper",
  "version": "1.0.0",
  "description": "Binds to a MagTek MSR (sureswipe) and parses onecard data to keystrokes",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . onecardSwiper --platform=win32 --arch=x64 --out=dist",
    "package-mac": "electron-packager . magtekOnecardSwiper --platform=darwin --arch=x64 --out=dist"
  },
  "author": "Dave Luhman",
  "license": "ISC",
  "dependencies": {
    "node-gyp": "^10.2.0",
    "node-hid": "^3.1.0",
    "npm": "^10.8.2"
  },
  "devDependencies": {
    "electron": "^31.3.1",
    "electron-packager": "^17.1.2"
  }
}